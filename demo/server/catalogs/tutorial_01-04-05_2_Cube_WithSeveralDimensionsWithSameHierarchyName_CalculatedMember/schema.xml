<Schema
  name="Minimal_Cubes_With_Two_Dimension_With_Same_Hierarchy_Name_Plus_One_Calculated_Member"
  description="dfsdfdsfdsf dsg">
  <Documentation>
		<![CDATA[
		A minimal cube with two dimension with same hierarchy name plus one calculated member.
		Calculated member should use hierarchy from Dimension1

		]]>
  </Documentation>

  <Dimension name="D0">
    <Hierarchy hasAll="true" name="Hierarchy1" primaryKey="KEY">
      <Table name="Fact"/>
      <Level name="Level" column="KEY" />
    </Hierarchy>
  </Dimension>

  <Dimension name="D1">
    <Hierarchy hasAll="true" name="Hierarchy1" primaryKey="KEY">
      <Table name="Fact"/>
      <Level name="Level" column="KEY" />
    </Hierarchy>
  </Dimension>

  <Cube name="CubeTwoMeasuresPlusOneCalculatedMeasure">
    <Table name="Fact" />
    <DimensionUsage name="Dimension0" source="D0" foreignKey="KEY"/>
    <DimensionUsage name="Dimension1" source="D1" foreignKey="KEY"/>
    <Measure aggregator="sum" name="Measure1-Sum" column="VALUE" />
    <Measure aggregator="count" name="Measure2-Count"
      column="VALUE" />
    <CalculatedMember name="CM2"
      hierarchy="[Dimension1.Hierarchy1]"
      formula="[Measures].[Measure1-Sum] / [Measures].[Measure2-Count]"
      parent="[Dimension1.Hierarchy1].[All Dimension1.Hierarchy1s]">
      <CalculatedMemberProperty name="FORMAT_STRING" value="0.0%"/>
    </CalculatedMember>
  </Cube>
</Schema>
