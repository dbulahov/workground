<?xml version="1.0" encoding="UTF-8"?>
<Schema name="Minimal_Cubes_With_Measures">

<!--


-->
  <Cube name="CubeOneMeasure">
    <Table name="Fact"/>
    <Measure aggregator="sum" name="Measure-Sum" column="VALUE"/>
  </Cube>
  
  <Cube name="CubeTwoMeasures">
    <Table name="Fact"/>
    <Measure aggregator="sum" name="Measure1-Sum" column="VALUE"/>
	<Measure aggregator="count" name="Measure2-Count" column="VALUE"/>
  </Cube>
  
  <Cube name="CubeTwoMeasuresPlusOneCalculatedMeasure">
    <Table name="Fact"/>
    <Measure aggregator="sum" name="Measure1-Sum" column="VALUE"/>
	<Measure aggregator="count" name="Measure2-Count" column="VALUE"/>
	<CalculatedMember name="Measure3-Calculated" dimension="Measures" formula="[Measures].[Measure1-Sum] / [Measures].[Measure2-Count]"/>
  </Cube>

  <Cube name="CubeOneNumericMeasureDifferentDataTypes">
    <Table name="Fact"/>
    <Measure aggregator="sum" name="Measure1-Integer" column="VALUE_NUMERIC" datatype="Integer"/>
	<Measure aggregator="sum" name="Measure2-Numeric" column="VALUE_NUMERIC" datatype="Numeric" formatString="#,###.00"/>
  </Cube>

</Schema>